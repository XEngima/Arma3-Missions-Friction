namespace Mission.Server
{
	public class ArrowConfig : IArrowConfig
	{
		// Creates a ArrowConfig object.
		public constructor()
		{
			_self.BlueArrowClassName = "Sign_Arrow_Large_Blue_F";
			_self.RedArrowClassName = "Sign_Arrow_Large_F";
		};

		// Gets the blue arrow's class name.
		public property String BlueArrowClassName { get; private set; };

		// Gets the blue arrow's class name.
		public property String RedArrowClassName { get; private set; };
	};
};
